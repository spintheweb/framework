{
	"name": "webspinner (Deno)",
	"image": "mcr.microsoft.com/devcontainers/base:jammy",
	"features": {
		"ghcr.io/devcontainers/features/deno:1": {
			"version": "latest"
		}
	},
	"postCreateCommand": "deno cache stwSpinner.ts",
	"forwardPorts": [8080],
	"portsAttributes": {
		"8080": {
			"label": "WebSpinner",
			"onAutoForward": "openBrowser",
			"visibility": "public"
		}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"denoland.vscode-deno",
				"ms-azuretools.vscode-docker"
			],
			"settings": {
				"deno.enable": true,
				"deno.lint": true,
				"deno.suggest.imports.hosts": {
					"https://deno.land": true,
					"https://jsr.io": true
				}
			}
		}
	}
}
